      setUser(userEmail);\n      // console.log(user);\n    } else {\n      setRedirect(true);\n    }\n  };\n\n  const loadAllData = () => {\n    getData()\n      .then((data) => {\n        if (data.error) {\n          setError(true);\n        } else {\n          setData(data);\n          setLoading(false);\n        }\n      })\n      .catch((err) => {\n        setError(true);\n      });\n  };\n\n  const loadHomeComponent = () => {\n    return (\n      <div>\n        <h2 className=\"text-center text-white\">Home</h2>\n        <br />\n        <div className=\"container\">\n          <div className=\"row\">\n            <div className=\"col-md-4 col-sm-0\">\n              <div className=\"card\">\n                <div\n                  className=\"card-header text-center\"\n                  style={{ fontWeight: \"bold\" }}\n                >\n                  {data.fileName}\n                </div>\n                <div className=\"card-body\">\n                  <p className=\"card-text\">\n                    <span style={{ fontWeight: \"bold\" }}>\n                      This is the actual file available for download.\n                    </span>\n                    <br />\n                    The content of the file is shown in the TextBox\n                  </p>\n                  <div className=\"text-right\">\n                    <a\n                      href={`https://codeshare.pythonanywhere.com/${data.fileName}`}\n                      download={data.fileName}\n                      target=\"_blank\"\n                    >\n                      <button className=\"btn btn-outline-primary\">\n                        Download\n                      </button>\n                    </a>\n                  </div>\n                </div>\n              </div>\n              <br />\n            </div>\n            <div className=\"col-md-8 col-sm-12\">\n              <h5 className=\"text-white\">Content of the File</h5>\n              <textarea id=\"content\" value={data.content}></textarea>\n            </div>\n          </div>\n        </div>\n      </div>\n    );\n  };\n\n  const redirectToLogin = () => {\n    return <Redirect to=\"/login\" />;\n  };\n\n  const showError = () => {\n    return (\n      <div className=\"container text-center\">\n        <br />\n        <br />\n        <br />\n        <br />\n        <img id=\"errorImg\" width={\"50%\"} src={logo} alt=\"error\" />\n        <br />\n        <br />\n        <h6 className=\"text-warning\" style={{ fontWeight: \"bold\" }}>\n          Something Went Wrong!\n        </h6>\n      </div>\n    );\n  };\n\n  const showLoading = () => {\n    return (\n      <div className=\"container text-center\">\n        <br />\n        <br />\n        <br />\n        <br />\n        <br />\n        <br />\n        <div className=\"spinner-border text-warning\" role=\"status\">\n          <span className=\"sr-only\">Loading...</span>\n        </div>\n      </div>\n    );\n  };\n\n  useEffect(() => {\n    authStatus();\n    loadAllData();\n  }, []);\n\n  return (\n    <Base userEmail={user}>\n      <br />\n      {error ? showError() : loading ? showLoading() : loadHomeComponent()}\n      {redirect ? redirectToLogin() : \"\"}\n    </Base>\n  );\n};\n\nexport default Home;\n","/home/shouvik/Desktop/Code Share/code-share/src/core/Form.js",[],"/home/shouvik/Desktop/Code Share/code-share/src/reusables/Base.js",[],"/home/shouvik/Desktop/Code Share/code-share/src/reusables/Navbar.js",[],"/home/shouvik/Desktop/Code Share/code-share/src/core/Login.js",[],"/home/shouvik/Desktop/Code Share/code-share/src/core/apicalls/apicalls.js",[],"/home/shouvik/Desktop/Code Share/code-share/src/Backend.js",[],"/home/shouvik/Desktop/Code Share/code-share/src/core/Live.js",[],{"ruleId":"51","replacedBy":"52"},{"ruleId":"53","replacedBy":"54"},{"ruleId":"55","severity":1,"message":"56","line":68,"column":21,"nodeType":"57","endLine":72,"endColumn":22},"no-native-reassign",["58"],"no-negated-in-lhs",["59"],"r